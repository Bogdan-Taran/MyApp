<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <gradient
                android:type="linear"
                android:startColor="#00A2E1"
                android:endColor="#1999CB"
                android:angle="270"
                />
            <stroke android:color="@color/black"
                android:width="2sp"/>


        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient
                android:type="linear"
                android:startColor="#056C9A"
                android:endColor="#215076"
                android:angle="270"
                />
            <stroke android:color="@color/black"
                android:width="2sp"/>



        </shape>




    </item>


</selector>