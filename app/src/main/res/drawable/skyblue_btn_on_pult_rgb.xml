<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <gradient
                android:type="linear"
                android:startColor="#2DC6E7"
                android:endColor="#46EA86"
                android:angle="270"
                />
            <stroke android:color="@color/black"
                android:width="2sp"/>


        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient
                android:type="linear"
                android:startColor="#1E849A"
                android:endColor="#309D5A"
                android:angle="270"
                />
            <stroke android:color="@color/black"
                android:width="2sp"/>



        </shape>




    </item>


</selector>