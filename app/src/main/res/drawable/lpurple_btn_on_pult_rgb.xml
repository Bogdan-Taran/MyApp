<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <gradient
                android:type="linear"
                android:startColor="#A1F54CB2"
                android:endColor="#D9950052"
                android:angle="270"
                />

            <stroke android:color="@color/black"
                android:width="2sp"/>


        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient
                android:type="linear"
                android:startColor="#A18C2C66"
                android:endColor="#D937011F"
                android:angle="270"
                />

            <stroke android:color="@color/black"
                android:width="2sp"/>



        </shape>




    </item>


</selector>