<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="0sp"
        android:left="0sp"
        android:right="0sp"
        android:top="0sp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="2dp"
                android:color="#44B6EC" />

            <gradient
                android:type="linear"
                android:startColor="#EB4DB3CB"
                android:endColor="#F5F49BFB"
                android:angle="90"
                />


        </shape>
    </item>

</layer-list>